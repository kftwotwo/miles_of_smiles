<%= form_for @product do |f| %>
    <%= f.hidden_field :user_id, :value => current_user.id %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :price %>
    <%= f.text_field :price %>
    <%= f.submit 'Create' %>
<% end %>
